{
  "name": "solium",
  "version": "1.1.7",
  "description": "Linter to identify and fix Style & Security issues in Solidity",
  "main": "./lib/solium.js",
  "bin": {
    "solium": "./bin/solium.js"
  },
  "scripts": {
    "test": "nyc --reporter=text --reporter=html mocha --require should --reporter spec --recursive",
    "lint": "node_modules/prettier/bin-prettier.js --write \"lib/**/*.js\" --write \"test/**/*.js\" --write \"bin/**/*.js\" --write \"config/**/*.js\"",
    "precommit": "pretty-quick --staged"
  },
  "keywords": [
    "lint",
    "static-analysis",
    "solidity",
    "abstract-syntax-tree",
    "ethereum",
    "smart-contracts",
    "solium",
    "blockchain",
    "code-quality",
    "dapp",
    "developer-tools",
    "security",
    "soliumconfig",
    "soliumplugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duaraghav8/Solium.git"
  },
  "author": "Raghav Dua <duaraghav8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ajv": "^5.2.2",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "js-string-escape": "^1.0.1",
    "lodash": "^4.14.2",
    "sol-digger": "0.0.2",
    "sol-explore": "1.6.1",
    "solium-plugin-security": "0.1.1",
    "solparse": "2.2.5",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^4.13.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "mocha": "^3.4.2",
    "nyc": "^11.2.1",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "should": "^11.0.0",
    "solium-config-test": "0.0.1",
    "solium-config-test-invalid-schema": "0.0.0",
    "solium-config-test-invalid-syntax": "0.0.0",
    "solium-plugin-test": "0.1.5",
    "solium-plugin-test-invalid-schema": "0.0.0"
  }
}
